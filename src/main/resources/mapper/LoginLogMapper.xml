<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daniu.mapper.LoginLogMapper">

    <resultMap id="BaseResultMap" type="com.daniu.model.entity.LoginLog">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="userName" column="user_name" jdbcType="VARCHAR"/>
            <result property="loginTime" column="login_time" jdbcType="TIMESTAMP"/>
            <result property="loginIp" column="login_ip" jdbcType="VARCHAR"/>
            <result property="loginAddr" column="login_addr" jdbcType="VARCHAR"/>
            <result property="driverName" column="driver_name" jdbcType="VARCHAR"/>
            <result property="osName" column="os_name" jdbcType="VARCHAR"/>
            <result property="loginState" column="login_state" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,user_name,login_time,
        login_ip,login_addr,driver_name,
        os_name,login_state
    </sql>
</mapper>
